let raceNumber = Math.floor(Math.random() * 1000);
let registeredEarly = false;
const runnerAge = 18;

if (registeredEarly && runnerAge > 18) {
raceNumber = raceNumber + 1000;
console.log(raceNumber);
} if (runnerAge > 18 && registeredEarly) {
  console.log('Hello racer number ' + raceNumber + '! Your start time is 9:30 AM.')
} if (runnerAge > 18 && registeredEarly === false) {
  console.log('Hello racer number ' + raceNumber + '! Your start time is 11:00 AM.')
} if (runnerAge < 18) {
  console.log('Hello racer number ' + raceNumber + '! Your start time is 12:30 PM.')
} else {
  console.log('Please see the front desk.')
}
